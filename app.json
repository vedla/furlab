{
  "expo": {
    "name": "Furlab",
    "slug": "furlab",
    "version": "1.0.0",
    "scheme": "furlab",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "developmentClient": {
      "silentLaunch": true
    },
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#241b45"
    },
    "backgroundColor": "#241b45",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.vedla.furlab",
      "icon": "./src/assets/icon/ios/AppIcon~ios-marketing.png",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "icon": "./src/assets/icon/android/res/mipmap-xxxhdpi/ic_launcher.png",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon/android/res/mipmap-xxxhdpi/ic_launcher_foreground.png",
        "backgroundImage": "./src/assets/icon/android/res/mipmap-xxxhdpi/ic_launcher_background.png",
        "monochromeImage": "./src/assets/icon/android/res/mipmap-xxxhdpi/ic_launcher_monochrome.png"
      },
      "package": "io.vedla.furlab"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#241b45",
          "image": "/src/assets/icon.png",
          "dark": {
            "image": "/src/assets/icon.png",
            "backgroundColor": "#241b45"
          },
          "imageWidth": 200
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID."
        }
      ],
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-router",
      "expo-localization",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID for easy app unlock."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos so you can pick the image you would like to search."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "483f880a-454b-47fe-ad30-b51301de1344"
      },
      "blurHash": "MDGID}4UIi_M5JyJ|P%zD}OaY6:8?c?E5Q"
    },
    "owner": "vedla",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}